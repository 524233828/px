{
  "name": "jose-chan/base-api",
  "description": "The api Base Class For Laravel Extension.",
  "keywords": ["laravel", "api base"],
  "authors": [{
    "name": "JoseChan",
    "email": "524233828@qq.com"
  }],
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "^7.1.3",
    "laravel/framework": "5.6.*"
  },
  "require-dev": {
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "JoseChan\\Base\\Api\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
